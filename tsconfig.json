{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["*.ts", "scripts/**/*.ts"],
  "exclude": ["node_modules", "sources", "vendor", "skills", "evals"]
}
